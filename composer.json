{
    "name": "wyrihaximus/get-in-packages-composer.jason",
    "description": "Functions around joshdifabio/composed and igorw/get-in to gather configuration from composer.json",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^8 || ^7.4",
        "igorw/get-in": "^1.0",
        "joshdifabio/composed": "^1.0"
    },
    "require-dev": {
        "wyrihaximus/test-utilities": "^5 || ^3.7.6"
    },
    "autoload": {
        "files": [
            "src/functions_include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "icanhazstring/composer-unused": true,
            "ergebnis/composer-normalize": true
        },
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
